	<queryString><![CDATA[SELECT HOSPITAL.COMPANY_NAME,DOCTOR_PROFILE.CODE,NAME_THAI,POSITION,BRANCH,SUB_BRANCH,DESCRIPTION,FROM_DATE,DR_NET_PAID_AMT,SUMMARY_MONTHLY.MM,SUMMARY_MONTHLY.YYYY,BATCH.YYYY as batch_date
FROM SUMMARY_MONTHLY
LEFT JOIN DOCTOR_PROFILE ON DOCTOR_PROFILE.CODE = SUMMARY_MONTHLY.DOCTOR_CODE
LEFT JOIN HOSPITAL ON HOSPITAL.CODE = DOCTOR_PROFILE.HOSPITAL_CODE
LEFT JOIN BATCH ON BATCH.HOSPITAL_CODE = HOSPITAL.CODE
LEFT JOIN DEPARTMENT ON DEPARTMENT.CODE = DOCTOR_PROFILE.DEPARTMENT_CODE
WHERE SUMMARY_MONTHLY.YYYY+SUMMARY_MONTHLY.MM BETWEEN $P{startYYYY}+$P{startMM} AND $P{endYYYY}+$P{endMM}    AND DOCTOR_PROFILE.CODE = $P{doctor_code} AND BATCH.CLOSE_DATE = ''
