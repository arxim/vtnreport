	</parameter>
	<parameter name="order_item_category" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT * FROM (
(SELECT 
		CASE WHEN INVOICE_DATE <= (SELECT CONVERT(varchar(6),(BATCH_NO-2))+'99' FROM BATCH WHERE HOSPITAL_CODE='VTN01' AND CLOSE_DATE ='') AND (SD.GUARANTEE_NOTE LIKE '%OLD%') AND DR_AMT=0 THEN 'N' ELSE 'S' END AS CHK,
		DP.CODE AS PROFILE_CODE,
		DP.NAME_THAI AS PROFILE_NAME,
		1 AS 'TYPE_QUERY',
		SD.VERIFY_DATE AS 'VERIFY_DATE',
		SD.DOCTOR_CODE AS 'DOCTOR_CODE',
		SD.VERIFY_TIME AS 'START_TIME',
		SD.VERIFY_DATE AS 'END_DATE',
		SD.VERIFY_TIME AS 'END_TIME',
		0.00 AS 'ABSORB_AMOUNT',
		SD.YYYY AS 'YYYY', 
		SD.MM AS 'MM', 
		DR.NAME_THAI AS 'NAME_THAI',
		DR.DOCTOR_CATEGORY_CODE,
		SD.INVOICE_NO AS 'INVOICE_NO', 
		SD.INVOICE_DATE AS 'INVOICE_DATE',
		SD.PATIENT_NAME AS 'PATIENT_NAME',
		SD.HN_NO AS 'HN_NO', 
		SD.IS_PAID, 
		SD.ADMISSION_TYPE_CODE,
		SD.AMOUNT_AFT_DISCOUNT AS 'OLD_AMOUNT',
		SD.AMOUNT_BEF_DISCOUNT-SD.AMOUNT_OF_DISCOUNT AS 'WRITE_OFF_AMOUNT', 
		SD.INVOICE_TYPE,
		SD.LINE_NO AS 'LINE_NO',
		OI.DESCRIPTION_THAI AS 'ORDER_ITEM_CODE', 
		OI.ORDER_ITEM_CATEGORY_CODE,
		CASE WHEN SD.MM = $P{month}
	 		THEN 
			SD.DR_AMT
	 		ELSE
			CASE WHEN SD.GUARANTEE_TERM_MM = $P{month} AND SD.GUARANTEE_TERM_YYYY = $P{year} 
			     AND SD.GUARANTEE_NOTE IN ('ABSORB SOME GUARANTEE','ABSORB OLD SOME')
				THEN SD.GUARANTEE_PAID_AMT
			 	ELSE SD.DR_AMT				
			END
		END AS 'DR_AMT',
		--SD.DR_AMT AS 'DR_AMT', 
		CASE WHEN GUARANTEE_NOTE LIKE '%EXTRA%' THEN 'EXTRA' ELSE 
	   CASE WHEN GUARANTEE_NOTE LIKE 'IN%' THEN 'IN' ELSE
	   CASE WHEN GUARANTEE_NOTE LIKE 'OVER%' THEN 'OVER' ELSE
	   CASE WHEN GUARANTEE_NOTE LIKE '%ABSORB GUARANTEE%' THEN GUARANTEE_NOTE ELSE 
	   CASE WHEN GUARANTEE_NOTE LIKE '%ABSORB SOME GUARANTEE%' THEN GUARANTEE_NOTE ELSE 
	   CASE WHEN GUARANTEE_NOTE LIKE '%ABSORB OLD GUARANTEE%' THEN GUARANTEE_NOTE ELSE
	   CASE WHEN GUARANTEE_NOTE LIKE '%ABSORB OLD SOME%' THEN GUARANTEE_NOTE ELSE 
	   '' END END END END END END END AS GUARANTEE_NOTE,
		SD.IS_WRITE_OFF AS 'IS_WRITE_OFF', 
		CASE WHEN SD.PAY_BY_AR = 'Y' THEN 'A' ELSE 'B' END AS PAY_METHOD,
		--SD.DESCRIPTION_THAI AS 'DESCRIPTION_THAI',
		CASE WHEN SD.MM = $P{month}
	 	     THEN SD.DR_TAX_406 
	 	     ELSE CASE WHEN SD.GUARANTEE_TERM_MM = $P{month} AND SD.GUARANTEE_TERM_YYYY = $P{year}
				    AND SD.GUARANTEE_NOTE IN ('ABSORB OLD SOME','ABSORB SOME GUARANTEE')
				    AND SD.DR_AMT > 0
			       THEN SD.GUARANTEE_PAID_AMT
			       ELSE SD.DR_TAX_406 END
		     END AS DR_TAX_AMT, SD.OLD_DR_AMT, SD.PRIVATE_DOCTOR
	FROM TRN_DAILY SD LEFT OUTER JOIN DOCTOR DR ON (SD.DOCTOR_CODE = DR.CODE
	AND SD.HOSPITAL_CODE = DR.HOSPITAL_CODE)
	LEFT OUTER JOIN DOCTOR_PROFILE DP ON (DR.DOCTOR_PROFILE_CODE = DP.CODE
	AND DR.HOSPITAL_CODE = DP.HOSPITAL_CODE)
	LEFT OUTER JOIN ORDER_ITEM OI ON (OI.CODE = SD.ORDER_ITEM_CODE
	AND OI.HOSPITAL_CODE = SD.HOSPITAL_CODE)
	WHERE (DP.CODE BETWEEN $P{from_doctor} AND $P{to_doctor})
	AND SD.DOCTOR_CODE IN (SELECT DOCTOR_CODE FROM PAYMENT_MONTHLY WHERE YYYY = $P{year}
	AND MM = $P{month})
	AND ((SD.YYYY LIKE $P{year} AND SD.MM LIKE $P{month}) 
	OR (SD.GUARANTEE_TERM_YYYY LIKE $P{year} AND SD.GUARANTEE_TERM_MM LIKE $P{month} AND SD.GUARANTEE_NOTE LIKE 'ABS%'))
	AND SD.HOSPITAL_CODE LIKE $P{hospital_code}
	AND DR.DOCTOR_CATEGORY_CODE LIKE $P{doctor_category}
	AND DR.DEPARTMENT_CODE LIKE $P{doctor_department}
	AND SD.ACTIVE = '1' AND SD.ORDER_ITEM_ACTIVE = '1'
	AND OI.IS_COMPUTE = 'Y'
	AND SD.ORDER_ITEM_CODE LIKE $P{order_item}
	AND OI.ORDER_ITEM_CATEGORY_CODE LIKE $P{order_item_category}
	AND SD.COMPUTE_DAILY_DATE is not null AND SD.COMPUTE_DAILY_DATE != ''
	--AND (SD.PAY_BY_CASH='Y' OR SD.PAY_BY_AR='Y' OR SD.PAY_BY_DOCTOR='Y' OR SD.PAY_BY_PAYOR='Y' OR SD.PAY_BY_CASH_AR='Y' )
	--AND SD.IS_PAID = 'Y'
	AND SD.AMOUNT_AFT_DISCOUNT > 0
	AND (SD.GUARANTEE_NOTE LIKE '%OLD%' OR SD.DR_AMT > 0)
	--AND SD.DR_AMT > 0
	)
UNION
	(SELECT top 3000
		'S' AS CHK,
		DP.CODE AS PROFILE_CODE,
		DP.NAME_THAI AS PROFILE_NAME,
		2 AS 'TYPE_QUERY',		
		STP.START_DATE AS 'VERIFY_DATE',
		STP.HP_ABSORB_DOCTOR_CODE AS 'DOCTOR_CODE',
		STP.START_TIME AS 'START_TIME',
		STP.END_DATE AS 'END_DATE',
		STP.END_TIME AS 'END_TIME',
		CASE WHEN HP402_ABSORB_AMOUNT > 0 
			THEN (HP402_ABSORB_AMOUNT)
			ELSE DF402_CASH_AMOUNT
		END AS 'ABSORB_AMOUNT',
		'' AS 'YYYY', 
		'' AS 'MM', 
		DR.NAME_THAI AS 'NAME_THAI', 
		'' AS 'DOCTOR_CATEGORY_CODE',
		'' AS 'INVOICE_NO',
		'' AS 'INVOICE_DATE',
		'' AS 'PATIENT_NAME',
		'' AS 'HN_NO', 
		'' AS 'IS_PAID', 
		'Z' AS 'ADMISSION_TYPE_CODE',
		0.00 AS 'OLD_AMOUNT', 
		0.00 AS 'WRITE_OFF_AMOUNT',
		CASE WHEN STP.GUARANTEE_EXCLUDE_AMOUNT > 0 
			THEN STP.GUARANTEE_TYPE_CODE+' : Extra Amount'
			ELSE STP.GUARANTEE_TYPE_CODE+' : Absorb Guarantee'
		END AS 'INVOICE_TYPE',
		STP.GUARANTEE_TYPE_CODE AS 'LINE_NO',
		'' AS 'ORDER_ITEM_CODE', 
		'' AS 'ORDER_ITEM_CATEGORY_CODE',
		0.00 AS 'DR_AMT', 
		'' AS 'GUARANTEE_NOTE',
		'N' AS 'IS_WRITE_OFF', 
		'B' AS 'PAY_METHOD',
		CASE WHEN HP402_ABSORB_AMOUNT > 0 
			THEN (HP402_ABSORB_AMOUNT)
			ELSE DF402_CASH_AMOUNT
		END AS 'DR_TAX_AMT',
		0.00 AS 'OLD_DR_AMT',
		'' AS 'PRIVATE_DOCTOR'
	FROM STP_GUARANTEE STP 
	LEFT OUTER JOIN DOCTOR DR ON (STP.GUARANTEE_DR_CODE = DR.CODE AND STP.HOSPITAL_CODE = DR.HOSPITAL_CODE)
	LEFT OUTER JOIN DOCTOR_PROFILE DP ON (DR.DOCTOR_PROFILE_CODE = DP.CODE AND
	DR.HOSPITAL_CODE = DP.HOSPITAL_CODE)
	WHERE (STP.DF402_CASH_AMOUNT+STP.HP402_ABSORB_AMOUNT+STP.DF406_HOLD_AMOUNT >0)
	AND STP.GUARANTEE_TYPE_CODE = 'DLY'
	AND (DR.DOCTOR_PROFILE_CODE BETWEEN $P{from_doctor} AND $P{to_doctor})
	AND STP.YYYY LIKE $P{year}
	AND STP.MM LIKE $P{month}
	AND STP.HOSPITAL_CODE LIKE $P{hospital_code}
	AND DR.DOCTOR_CATEGORY_CODE LIKE $P{doctor_category}
	AND DR.DEPARTMENT_CODE LIKE $P{doctor_department})
UNION
	(SELECT DISTINCT
		'S' AS CHK,
		DP.CODE AS PROFILE_CODE,
		DP.NAME_THAI AS PROFILE_NAME,
		2 AS 'TYPE_QUERY',		
		STP.YYYY + STP.MM +'01' AS 'VERIFY_DATE',
		STP.HP_ABSORB_DOCTOR_CODE AS 'DOCTOR_CODE',
		'' AS 'START_TIME',
		STP.YYYY+STP.MM+'31' AS 'END_DATE',
		'' AS 'END_TIME',
		CASE WHEN HP402_ABSORB_AMOUNT > 0 
			THEN (HP402_ABSORB_AMOUNT)
			ELSE DF402_CASH_AMOUNT
		END AS 'ABSORB_AMOUNT',
		'' AS 'YYYY', 
		'' AS 'MM', 
		DR.NAME_THAI AS 'NAME_THAI', 
		'' AS 'DOCTOR_CATEGORY_CODE',
		'' AS 'INVOICE_NO',
		'' AS 'INVOICE_DATE',
		'' AS 'PATIENT_NAME',
		'' AS 'HN_NO', 
		'' AS 'IS_PAID', 
		'Z' AS 'ADMISSION_TYPE_CODE',
		0.00 AS 'OLD_AMOUNT', 
		0.00 AS 'WRITE_OFF_AMOUNT',
		CASE WHEN STP.GUARANTEE_EXCLUDE_AMOUNT > 0 
			THEN STP.GUARANTEE_TYPE_CODE+' : Extra Amount'
			ELSE STP.GUARANTEE_TYPE_CODE+' : Absorb Guarantee'
		END AS 'INVOICE_TYPE',
		STP.GUARANTEE_TYPE_CODE AS 'LINE_NO',
		'' AS 'ORDER_ITEM_CODE', 
		'' AS 'ORDER_ITEM_CATEGORY_CODE',
		0.00 AS 'DR_AMT', 
		'' AS 'GUARANTEE_NOTE',
		'N' AS 'IS_WRITE_OFF', 
		'B' AS 'PAY_METHOD',
		CASE WHEN HP402_ABSORB_AMOUNT > 0 
			THEN (HP402_ABSORB_AMOUNT)
			ELSE DF402_CASH_AMOUNT
		END AS 'DR_TAX_AMT',
		0.00 AS 'OLD_DR_AMT',
		'' AS 'PRIVATE_DOCTOR'
	FROM STP_GUARANTEE STP 
	LEFT OUTER JOIN DOCTOR DR ON (STP.GUARANTEE_DR_CODE = DR.CODE AND STP.HOSPITAL_CODE = DR.HOSPITAL_CODE)
	LEFT OUTER JOIN DOCTOR_PROFILE DP ON (DR.DOCTOR_PROFILE_CODE = DP.CODE AND
	DR.HOSPITAL_CODE = DP.HOSPITAL_CODE)
	WHERE (STP.DF402_CASH_AMOUNT+STP.HP402_ABSORB_AMOUNT+STP.DF406_HOLD_AMOUNT >0)
	AND STP.GUARANTEE_TYPE_CODE <> 'DLY'
	AND (DR.DOCTOR_PROFILE_CODE BETWEEN $P{from_doctor} AND $P{to_doctor})
	AND STP.YYYY LIKE $P{year}
	AND STP.MM LIKE $P{month}
	AND STP.HOSPITAL_CODE LIKE $P{hospital_code}
	AND DR.DOCTOR_CATEGORY_CODE LIKE $P{doctor_category}
	AND DR.DEPARTMENT_CODE LIKE $P{doctor_department})
)TRN WHERE TRN.CHK='S'
ORDER BY PROFILE_CODE,DOCTOR_CODE ,PAY_METHOD , END_DATE , END_TIME ,TYPE_QUERY  , ADMISSION_TYPE_CODE ,  INVOICE_DATE  , INVOICE_NO ;]]></queryString>

