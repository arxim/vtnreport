	<queryString><![CDATA[SELECT S.DOCTOR_CODE, 
SUM(S.DR_AMT) AS DR_AMT,
SUM(S.DR_TAX_400+S.DR_TAX_401+S.DR_TAX_402+S.DR_TAX_406) AS DR_TAX_AMT
FROM TRN_DAILY S 
WHERE S.HOSPITAL_CODE = $P{hospital_code} AND 
(S.INVOICE_DATE BETWEEN $P{from_date} AND $P{to_date}) AND
S.DOCTOR_CODE = $P{doctor_code} AND INVOICE_TYPE <> 'ORDER' AND S.ACTIVE = '1' AND
S.YYYY = '' AND S.MM = '' AND
(S.BATCH_NO = '' OR S.BATCH_NO IS NULL) AND
S.IS_PAID <> 'N' AND S.ORDER_ITEM_ACTIVE = '1'
GROUP BY S.DOCTOR_CODE]]></queryString>

