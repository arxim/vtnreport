	<queryString><![CDATA[SELECT PM.DOCTOR_CODE, DR.NAME_THAI,
CASE WHEN PM.PAYMENT_MODE_CODE = 'B' THEN 'BANK' ELSE
(CASE WHEN PM.PAYMENT_MODE_CODE = 'CQ' THEN 'CHEQUE' ELSE 'NONE' END)END AS PAYMENT_MODE,
PM.PAYMENT_TERM_DATE, PM.DR_NET_PAID_AMT
FROM PAYMENT_MONTHLY PM LEFT OUTER JOIN DOCTOR DR ON PM.DOCTOR_CODE = DR.CODE
WHERE PM.YYYY LIKE $P{year} AND PM.MM LIKE $P{month}
AND PM.HOSPITAL_CODE LIKE $P{hospital_code}
AND PM.IS_HOLD = 'Y'
AND (PM.PAYMENT_TERM_DATE IS NOT NULL OR PM.PAYMENT_TERM_DATE = '')]]></queryString>

