	<queryString><![CDATA[SELECT DOCTOR.CODE, DOCTOR.NAME_THAI, TRN_EXPENSE_DETAIL.DOCTOR_CODE, TRN_EXPENSE_DETAIL.YYYY, 
TRN_EXPENSE_DETAIL.MM, TRN_EXPENSE_DETAIL.AMOUNT * TRN_EXPENSE_DETAIL.EXPENSE_SIGN as AMOUNT,
CASE WHEN TRN_EXPENSE_DETAIL.EXPENSE_SIGN = '-1' THEN AMOUNT ELSE 0 END AS CREDIT_AMOUNT,
CASE WHEN TRN_EXPENSE_DETAIL.EXPENSE_SIGN = '1' THEN AMOUNT ELSE 0 END AS DEBIT_AMOUNT,
TRN_EXPENSE_DETAIL.EXPENSE_CODE, TRN_EXPENSE_DETAIL.INVOICE_TYPE_DESCRIPTION,
TRN_EXPENSE_DETAIL.DOC_DATE, TRN_EXPENSE_DETAIL.DOC_NO, EXPENSE.CODE, EXPENSE.DESCRIPTION,
TRN_EXPENSE_DETAIL.NOTE, TRN_EXPENSE_DETAIL.EXPENSE_SIGN, TRN_EXPENSE_DETAIL.TAX_AMOUNT,
TRN_EXPENSE_DETAIL.TAX_TYPE_CODE
FROM TRN_EXPENSE_DETAIL LEFT OUTER JOIN DOCTOR ON TRN_EXPENSE_DETAIL.DOCTOR_CODE = DOCTOR.CODE
LEFT OUTER JOIN EXPENSE ON TRN_EXPENSE_DETAIL.EXPENSE_CODE = EXPENSE.CODE
where TRN_EXPENSE_DETAIL.HOSPITAL_CODE LIKE $P{hospital_code}
and (DOCTOR.DOCTOR_PROFILE_CODE BETWEEN $P{from_doctor} and $P{to_doctor})
and TRN_EXPENSE_DETAIL.MM like $P{month}
and TRN_EXPENSE_DETAIL.YYYY = $P{year}
AND DOCTOR.DOCTOR_CATEGORY_CODE like $P{doctor_category}
AND DOCTOR.DEPARTMENT_CODE like $P{doctor_department}
AND EXPENSE_SIGN like $P{expense_sign}
AND EXPENSE_ACCOUNT_CODE like $P{expense_account_code}
AND EXPENSE_CODE like $P{expense_code}
order by DOCTOR.CODE, TRN_EXPENSE_DETAIL.DOC_NO]]></queryString>

